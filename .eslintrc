{
  "extends": "airbnb-base",
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "rules": {
    "prettier/prettier": ["error"],
    "comma-dangle": 0,
    "max-len": 0,
  },
  "env": {
    "jest": true
  }
}
